---
interface Props {
    className?: string;
    width?: number;
    height?: number;
}

const { className, width = 24, height = 24 } = Astro.props;
---

<svg
    class={className}
    aria-hidden="true"
    xmlns="http://www.w3.org/2000/svg"
    width={width}
    height={height == 0 ? width : height}
    fill="none"
    viewBox="0 0 24 24"
>
    <path
        stroke="currentColor"
        stroke-linecap="round"
        stroke-linejoin="round"
        stroke-width="2"
        d="M6 18 17.94 6M18 18 6.06 6"></path>
</svg>
